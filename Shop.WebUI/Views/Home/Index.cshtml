@model Shop.WebUI.ViewModel.ProductListModel

<div class="row">
    <div class="col-md-3">
        <ul class="list-group">
          @await Component.InvokeAsync("CategoryList")
        </ul>
    </div>
    <div class="col-md-9">

        <partial name="_slider" />

        <div class="row">
            @foreach (var product in Model.Products)
            {
                <partial name="_product" for="@product" />}
        </div>

    </div>
</div>